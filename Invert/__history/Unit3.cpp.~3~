TForm3 *Form3;
//---------------------------------------------------------------------------
__fastcall TForm3::TForm3(TComponent* Owner)
	: TForm(Owner)
{
	for (int i = 0; i < Form1->StringGrid2->RowCount ; i++) {
	Series1->AddXY(sqrt(pow(StrToFloat(Form1->StringGrid2->Cells[1][i]),2)+pow(StrToFloat(Form1->StringGrid2->Cells[1][i]),2)) )
	}

}
